<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity UserSubscription.
    -->
    <changeSet id="20200508103400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="subscription_plan_id"
                                 baseTableName="user_subscription"
                                 constraintName="fk_user_subscription_subscription_plan_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subscription_plan"/>

        <addForeignKeyConstraint baseColumnNames="user_profile_id"
                                 baseTableName="user_subscription"
                                 constraintName="fk_user_subscription_user_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"/>

    </changeSet>
</databaseChangeLog>
