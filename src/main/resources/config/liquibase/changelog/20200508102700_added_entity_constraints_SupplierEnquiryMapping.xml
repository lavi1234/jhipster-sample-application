<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity SupplierEnquiryMapping.
    -->
    <changeSet id="20200508102700-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="enquiry_id"
                                 baseTableName="supplier_enquiry_mapping"
                                 constraintName="fk_supplier_enquiry_mapping_enquiry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="enquiry"/>

        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="supplier_enquiry_mapping"
                                 constraintName="fk_supplier_enquiry_mapping_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="supplier_enquiry_mapping"
                                 constraintName="fk_supplier_enquiry_mapping_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"/>

    </changeSet>
</databaseChangeLog>
