<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Offer.
    -->
    <changeSet id="20200508101900-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="supplier_enquiry_id"
                                 baseTableName="offer"
                                 constraintName="fk_offer_supplier_enquiry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supplier_enquiry_mapping"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="offer"
                                 constraintName="fk_offer_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"/>

        <addForeignKeyConstraint baseColumnNames="updated_by_id"
                                 baseTableName="offer"
                                 constraintName="fk_offer_updated_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"/>

    </changeSet>
</databaseChangeLog>
