<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity OfferPrice.
    -->
    <changeSet id="20200508102200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="offer_id"
                                 baseTableName="offer_price"
                                 constraintName="fk_offer_price_offer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="offer"/>

        <addForeignKeyConstraint baseColumnNames="enquiry_id"
                                 baseTableName="offer_price"
                                 constraintName="fk_offer_price_enquiry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="enquiry"/>

        <addForeignKeyConstraint baseColumnNames="enquiry_details_id"
                                 baseTableName="offer_price"
                                 constraintName="fk_offer_price_enquiry_details_id"
                                 referencedColumnNames="id"
                                 referencedTableName="enquiry_details"/>

    </changeSet>
</databaseChangeLog>
